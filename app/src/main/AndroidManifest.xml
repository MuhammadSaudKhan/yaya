<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.saud.app.yaya">

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ActivityTimer"></activity>
        <activity android:name=".ActivityExercise" />
        <activity android:name=".ActivityAddExercise" />
        <activity android:name=".ActivityAddWorkout" />
        <activity android:name=".ActivityOptions" />
        <activity android:name=".ActivityFood" />
        <activity android:name=".ActivityCalender" />
        <activity android:name=".ActivityProgress" />
        <activity android:name=".ActivityProfile" />
        <activity android:name=".ActivityWorkout" />
        <activity
            android:name=".ActivityMain"
            android:theme="@style/AppTheme" />
        <service android:name=".ActivityTimer$BroadcastService" />
        <activity
            android:name=".SplashScreen"
            android:theme="@style/SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>